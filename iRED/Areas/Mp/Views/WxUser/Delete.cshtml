@model iRED.ViewModel.Mp.WxUserVMs.WxUserVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.OpenId" />
<wt:display field="Entity.NickName" />
<wt:display field="Entity.AvatarUrl" />
<wt:display field="Entity.Gender" />
<wt:display field="Entity.Country" />
<wt:display field="Entity.Province" />
<wt:display field="Entity.City" />
<wt:display field="Entity.Language" />
<wt:display field="Entity.CreateTime" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>